

<!-- MAIN SECTION -->
<main>
  <a href="./new" type="button" name="button" class="newArticleButton">
    New posting
  </a>
  <section>

    {{#each postings}}
    <article class="articleArea">
      <h3>{{title}}</h3>
      <h4>posted by: {{user_id}}.</h4>
      <h5>Date posted: {{date}}</h5>
      <p>{{body}}</p>
    </article>
    {{/each}}


  </section>
</main>




